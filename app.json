{
  "name": "kitchen-sink-line-bot",
  "description": "kitchen-sink LINE Bot for Heroku",
  "env": {
    "LINE_CHANNEL_ACCESS_TOKEN": {
      "description": "LINE アクセストークン（ロングターム）",
      "value":"",
      "required":true
    },
    "LINE_CHANNEL_SECRET": {
      "description": "LINE Channel Secret",
      "value":"",
      "required":true
    },
    "HEROKU_BASE_URL": {
      "description": "Base URL",
      "value":"https://{HEROKU_APP_NAME}.herokuapp.com",
      "required":true
    },
    "HEROKU_PORT": {
      "description": "Port",
      "value":"1234",
      "required":false
    },
    "addons": [
      "einstein-vision"
    ]
  }
}